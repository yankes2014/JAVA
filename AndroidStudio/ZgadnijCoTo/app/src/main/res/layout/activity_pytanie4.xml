<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_pytanie4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.domin.zgadnijcoto.pytanie4"
    android:background="@drawable/tlo">

    <Button
        android:text="2017"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/button25"
        android:layout_alignLeft="@+id/button20"
        android:layout_alignStart="@+id/button20"
        android:id="@+id/button26"
        android:onClick="ShowDalej" />

    <TextView
        android:text="Pytanie4"
        android:layout_width="140dp"
        android:layout_height="30dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="123dp"
        android:layout_marginStart="123dp"
        android:layout_marginTop="68dp"
        android:id="@+id/textView11"
        android:textSize="24sp" />

    <Button
        android:text="Menu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:id="@+id/button27"
        android:onClick="Cofnij" />

    <Button
        android:id="@+id/button28"
        android:text="Poprawna odpowiedz Dalej"
        android:layout_width="140dp"
        android:layout_height="70dp"
        android:layout_marginRight="19dp"
        android:layout_marginEnd="19dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="11dp"
        android:visibility="invisible"
        android:onClick="Dalej" />

    <TextView
        android:text="Ktory mamy rok?"
        android:layout_width="170dp"
        android:layout_height="75dp"
        android:layout_marginLeft="27dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="30dp"
        android:id="@+id/textView12"
        android:layout_below="@+id/textView11"
        android:layout_alignLeft="@+id/button27"
        android:layout_alignStart="@+id/button27"
        android:background="@drawable/nowe"
        android:textSize="14sp" />

    <Button
        android:text="2014"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button25"
        android:layout_above="@+id/button28"
        android:layout_alignLeft="@+id/button19"
        android:layout_alignStart="@+id/button19"
        android:layout_marginBottom="39dp"
        android:onClick="ZlaTrzy" />

    <Button
        android:text="2015"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button19"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/textView12"
        android:layout_alignStart="@+id/textView12"
        android:layout_marginLeft="22dp"
        android:layout_marginStart="22dp"
        android:onClick="ZlaRaz" />
    <Button
        android:text="Koniec Gry"
        android:layout_width="140dp"
        android:layout_height="70dp"
        android:layout_marginRight="19dp"
        android:layout_marginEnd="19dp"
        android:id="@+id/koniec"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="11dp"
        android:visibility="invisible"
        android:onClick="Koniec" />

    <Button
        android:text="2016"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button20"
        android:layout_alignBaseline="@+id/button19"
        android:layout_alignBottom="@+id/button19"
        android:layout_toRightOf="@+id/textView12"
        android:layout_toEndOf="@+id/textView12"
        android:layout_marginLeft="15dp"
        android:layout_marginStart="15dp"
        android:onClick="ZlaDwa" />
</RelativeLayout>
