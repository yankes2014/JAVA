<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_pytanie2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.domin.zgadnijcoto.pytanie2"
    android:background="@drawable/tlo">

    <TextView
        android:text="Pytanie2"
        android:layout_width="140dp"
        android:layout_height="30dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="123dp"
        android:layout_marginStart="123dp"
        android:layout_marginTop="68dp"
        android:id="@+id/textView7"
        android:textSize="24sp" />

    <TextView
        android:text="Jak ma na drugie imie Dominik?"
        android:layout_width="170dp"
        android:layout_height="75dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="111dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="128dp"
        android:id="@+id/textView8"
        android:background="@drawable/nowe" />

    <Button
        android:text="Oliwier"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="12dp"
        android:id="@+id/button12"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/textView8"
        android:layout_alignStart="@+id/textView8"
        android:onClick="ZlaRaz" />

    <Button
        android:text="Brajan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button13"
        android:layout_alignRight="@+id/button12"
        android:layout_alignEnd="@+id/button12"
        android:layout_marginTop="52dp"
        android:id="@+id/button14"
        android:onClick="ZlaDwa" />

    <Button
        android:text="Mariusz"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/button14"
        android:layout_alignLeft="@+id/button13"
        android:layout_alignStart="@+id/button13"
        android:id="@+id/button15"
        android:onClick="ZlaTrzy" />

    <Button
        android:text="Menu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:onClick="Cofnij"
        android:id="@+id/cofnij" />

    <Button
        android:id="@+id/dalej"
        android:text="Poprawna Odpowiedz Dalej"
        android:layout_width="140dp"
        android:layout_height="70dp"
        android:layout_marginRight="19dp"
        android:layout_marginEnd="19dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="11dp"
        android:visibility="invisible"
        android:onClick="Dalej" />

    <Button
        android:text="Koniec Gry"
        android:layout_width="140dp"
        android:layout_height="70dp"
        android:layout_marginRight="19dp"
        android:layout_marginEnd="19dp"
        android:id="@+id/koniec"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="11dp"
        android:visibility="invisible"
        android:onClick="Koniec" />

    <Button
        android:text="Jakub"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button13"
        android:onClick="ShowDalej"
        android:layout_alignBaseline="@+id/button12"
        android:layout_alignBottom="@+id/button12"
        android:layout_toRightOf="@+id/textView8"
        android:layout_toEndOf="@+id/textView8" />
</RelativeLayout>
